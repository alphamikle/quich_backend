{
  "name": "quich-server-app",
  "version": "2.3.0",
  "description": "Backend of Quich application API SPECS",
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=13"
  },
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "build:mem": "rimraf dist && node --max-old-space-size=2048 node_modules/typescript/bin/tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/src/main.js\"",
    "tsnode:dev": "ts-node-dev src/main.ts",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/src/main.js\"",
    "start:prod": "node dist/src/main.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:runner": "ts-node command-runner.ts"
  },
  "dependencies": {
    "@grpc/proto-loader": "^0.5.4",
    "@nestjs/common": "^7.0.11",
    "@nestjs/core": "^7.0.11",
    "@nestjs/microservices": "^7.0.11",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.11",
    "@nestjs/swagger": "^4.5.5",
    "@nestjs/typeorm": "^7.0.0",
    "@types/request-promise": "^4.1.46",
    "axios": "^0.19.2",
    "bcrypt": "^4.0.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "date-fns": "^2.13.0",
    "dotenv": "^8.2.0",
    "googleapis": "^50.0.0",
    "grpc": "^1.24.2",
    "html-entities": "^1.3.1",
    "jest-mock": "^26.0.1",
    "leven": "^3.1.0",
    "module-alias": "^2.2.2",
    "nest-transact": "^1.0.2",
    "node-html-parser": "^1.2.16",
    "nodemailer": "^6.4.6",
    "passport": "^0.4.1",
    "passport-http-bearer": "^1.0.1",
    "pg": "^8.1.0",
    "protoc-gen-grpc": "^1.4.0",
    "puppeteer": "^3.0.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "swagger-ui-express": "^4.1.4",
    "ts-proto": "^1.20.2",
    "typeorm": "^0.2.24",
    "uuid": "^8.0.0",
    "rxjs": "^6.5.5",
    "@ntegral/nestjs-sentry": "^1.0.3"
  },
  "devDependencies": {
    "@nestjs/testing": "^7.0.11",
    "@types/bcrypt": "^3.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "4.17.6",
    "@types/html-entities": "^1.2.16",
    "@types/jest": "25.2.1",
    "@types/node": "13.13.5",
    "@types/nodemailer": "^6.4.0",
    "@types/passport-http-bearer": "^1.0.35",
    "@types/puppeteer": "^2.1.0",
    "@types/supertest": "2.0.9",
    "@types/uuid": "^7.0.3",
    "@typescript-eslint/eslint-plugin": "^2.32.0",
    "@typescript-eslint/parser": "^2.32.0",
    "eslint": "^7.0.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "jest": "26.0.1",
    "prettier": "2.0.5",
    "supertest": "4.0.2",
    "ts-jest": "25.5.1",
    "ts-node": "^8.10.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsc-watch": "4.2.3",
    "tsconfig-paths": "3.9.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "_moduleAliases": {
    "~": "src",
    "@": "proto"
  }
}
